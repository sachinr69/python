#cp or sp
cp=int(input("Enter the cp value :"))
sp=int(input("Enter the sp value"))

if sp>cp:
    profit=sp-cp
    print("profit",profit)

elif sp<cp:
    loss=cp-sp
    print("loss",loss)

else:
    print( "no profit no loss")
